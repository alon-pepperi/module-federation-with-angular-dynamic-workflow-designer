{
  "name": "mf-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:shell": "ng serve shell  --disable-host-check --port 4400 --deploy-url http://localhost:4400/",
    "start:mfe1": "ng serve mfe1",
    "start:mfe2": "ng serve mfe2",
    "build:shell": "ng build shell --prod --output-hashing=none --sourceMap=true",
    "build:mfe1": "ng build mfe1 --prod",
    "build:mfe2": "ng build mfe2 --prod",
    "start": "concurrently \"npm run start:shell\" \"npm run start:mfe1\" \"npm run start:mfe2\" ",
    "build": "npm run build:shell && npm run build:mfe1 && npm run build:mfe2",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:single-spa:mfe1": "ng build mfe1 --prod --deploy-url http://localhost:4200/",
    "serve:single-spa:mfe1": "ng s --project mfe1 --disable-host-check --port 4200 --deploy-url http://localhost:4200/ --live-reload false",
    "build:single-spa:mfe2": "ng build mfe2 --prod --deploy-url http://localhost:4201/",
    "serve:single-spa:mfe2": "ng s --project mfe2 --disable-host-check --port 4201 --deploy-url http://localhost:4201/ --live-reload false"
  },
  "private": true,
  "resolutions": {
    "webpack": "^5.0.0"
  },
